+import customtkinter
+from styles import primary_color, option_button_color
+from PIL import Image
+
+
+class OptionsButtonFrame(customtkinter.CTkFrame):
+    def __init__(self, master, button_names, image_paths):
+        super().__init__(master, fg_color="transparent")
+        self.button_names = button_names
+        self.image_paths = image_paths
+        self.buttons = []
+        images = []
+
+        for i in self.image_paths:
+            image = customtkinter.CTkImage(light_image=Image.open(image_paths[i]),
+                                           dark_image=Image.open(image_paths[i]),
+                                           size=(40,40))
+            images.append(image)
+
+
+        for i, name in enumerate(self.button_names):
+            button = customtkinter.CTkButton(self, text=name, height=60, width=100, fg_color="transparent", border_color=option_button_color,
+                                             border_width=4, hover_color=option_button_color, image=)
+            button.grid(row=i, column=0, sticky="w", padx=10, pady=(0,20))
+            self.buttons.append(button)
+
+
+
+
+
+class ExpenseTrackingApp(customtkinter.CTk):
+    def __init__(self):
+        super().__init__()
+        self.title('Expense Tracking')
+        self.geometry('800x600')
+        self.columnconfigure(0, weight=1)
+        self.rowconfigure(0, weight=1)
+
+        options_frame = customtkinter.CTkFrame(self, fg_color=primary_color)
+        options_frame.grid(row=0, column=0, sticky="nsw", padx=10, pady=10)
+        self.options_button_frame = OptionsButtonFrame(options_frame, ["Accounts", "Debts", "Economy"])
+        self.options_button_frame.grid(row=0, column=0, sticky="nsew", pady=(20,10))
+
+app = ExpenseTrackingApp()
+app.mainloop()
+
